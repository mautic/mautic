    "liip/functional-test-bundle": "^4.2",
    "php-http/mock-client": "^1.4"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mautic/BazingaOAuthServerBundle.git"
    },
    {
      "type": "git",
      "url": "https://github.com/dennisameling/FOSOAuthServerBundle.git"
    },
    {
      "type": "package",
      "package": {
        "name": "mautic/plugin-grapesjs-builder",
        "version": "1.0-RC4",
        "type": "mautic-plugin",
        "source": {
          "url": "https://github.com/mautic/plugin-grapesjs-builder.git",
          "type": "git",
          "reference": "1.0-RC4"
        },
        "require": {
          "composer/installers": "~1.0"
        },
        "extra": {
          "installer-name": "GrapesJsBuilderBundle"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "composer/installers",
        "version": "1.10",
        "type": "composer-plugin",
        "source": {
          "url": "https://github.com/nickveenhof/installers.git",
          "type": "git",
          "reference": "patch-2"
        }
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "autoloader-suffix": "Mautic4"
  },
  "extra": {
    "mautic-scaffold": {
      "file-mapping": {
        "[project-root]/.env.dist": "assets/scaffold/files/.env.dist",
        "[project-root]/Gruntfile.js": "assets/scaffold/files/Gruntfile.js",
        "[project-root]/package.json": "assets/scaffold/files/package.json",
        "[project-root]/package-lock.json": "assets/scaffold/files/package-lock.json",
        "[project-root]/bin/console": "assets/scaffold/files/console",
        "[project-root]/bin/.htaccess": "assets/scaffold/files/deny.htaccess",
        "[web-root]/.htaccess": "assets/scaffold/files/htaccess",
        "[web-root]/.gitignore": "assets/scaffold/files/example.gitignore",
        "[web-root]/index.php": "assets/scaffold/files/index.php",
        "[web-root]/index_dev.php": "assets/scaffold/files/index_dev.php",
        "[web-root]/offline.php": "assets/scaffold/files/offline.php",
        "[web-root]/robots.txt": "assets/scaffold/files/robots.txt",
        "[web-root]/upgrade.php": "assets/scaffold/files/upgrade.php",
        "[web-root]/translations/.htaccess": "assets/scaffold/files/translations.htaccess",
        "[web-root]/favicon.ico": "assets/scaffold/files/favicon.ico",
        "[web-root]/media/dashboards/alternative-default.json": "assets/scaffold/media/dashboards/alternative-default.json",
        "[web-root]/media/dashboards/default.json": "assets/scaffold/media/dashboards/default.json",
        "[web-root]/media/dashboards/email-overview.json": "assets/scaffold/media/dashboards/email-overview.json",
        "[web-root]/media/dashboards/stats-overview.json": "assets/scaffold/media/dashboards/stats-overview.json",
        "[web-root]/media/files/.htaccess": "assets/scaffold/media/files/.htaccess",
        "[web-root]/media/images/.htaccess": "assets/scaffold/media/images/.htaccess",
        "[web-root]/themes/blank.png": "assets/scaffold/files/blank.png",
        "[web-root]/themes/blank-big.png": "assets/scaffold/files/blank-big.png",
        "[project-root]/app/.htaccess": "assets/scaffold/files/deny.htaccess"
      }
    }
  },
  "scripts": {
    "pre-autoload-dump": "Mautic\\ComposerBundle\\ComposerBundle::preAutoloadDump"
  }
}