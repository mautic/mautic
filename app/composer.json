{
  "name": "mautic/core-lib",
  "license": "GPL-3.0",
  "type": "mautic-core",
  "description": "Mautic Open Source Distribution",
  "autoload": {
    "psr-4": {
      "Mautic\\": "bundles/",
      "MauticPlugin\\": "plugins/",
      "Mautic\\Middleware\\": "middlewares/",
      "Mautic\\Migrations\\": "migrations/",
      "Mautic\\CodingStandards\\": "codingstandards/"
    },
    "files": [
      "AppKernel.php",
      "AppCache.php"
    ]
  },
  "require": {
    "php": "~8.0",
    "ext-zlib": "*",
    "ext-zip": "*",
    "ext-imap": "*",
    "ext-pdo": "*",
    "symfony/amqp-messenger": "~5.4",
    "symfony/console": "~5.4.0",
    "symfony/form": "~5.4.0",
    "symfony/intl": "~5.4.0",
