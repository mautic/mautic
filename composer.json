{
    "name": "mautic/core",
    "license": "GPL-3.0",
    "type": "mautic-core",
    "description": "Mautic Open Source Distribution",
    "autoload": {
        "psr-4": {
            "Mautic\\": "app/bundles/",
            "MauticPlugin\\": "plugins/",
            "Mautic\\Middleware\\": "app/middlewares/",
            "Mautic\\Migrations\\": "app/migrations/"
        },
        "files": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
